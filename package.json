{
  "name": "vue-roast",
  "version": "0.3.0",
  "description": "Vue.js plugin for easy display of toast messages",
  "main": "dist/vue-roast.min.js",
  "module": "dist/vue-roast.es.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "dev": "poi",
    "build": "npm run clean:dist && bili",
    "docs": "npm run clean:docs && poi build",
    "clean": "npm run clean:dist && npm run clean:docs",
    "clean:dist": "del-cli dist/* !dist/.gitkeep",
    "clean:docs": "del-cli docs/* !docs/.gitkeep !docs/CNAME",
    "release": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kronicker/vue-roast.git"
  },
  "keywords": [
    "vue",
    "toast",
    "message",
    "vuejs",
    "javascript",
    "js",
    "notification",
    "popup"
  ],
  "author": {
    "name": "Toma Zelic",
    "email": "zelic.toma@gmail.com",
    "url": "https://github.com/kronicker"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://ub.com/kronicker/vue-roast/issues"
  },
  "devDependencies": {
    "bili": "^3.0.7",
    "del-cli": "^1.1.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass": "^4.7.2",
    "poi": "^9.6.13",
    "rollup-plugin-vue": "^3.0.0",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "wingcss": "^1.0.0-beta"
  }
}
